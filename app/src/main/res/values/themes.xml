<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TakeNote" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="android:colorPrimary">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="android:colorSecondary">@color/black</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >@color/dark_gray</item>
        <!-- Customize your theme here. -->


    </style>

    <style name="Theme.TakeNote.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Theme.CleanArchTodoAppPrep.AppBarOverlay" parent="Theme.AppCompat.DayNight.DarkActionBar" />

    <style name="Theme.CleanArchTodoAppPrep.PopupOverlay" parent="Theme.AppCompat.Light.DarkActionBar" />

</resources>